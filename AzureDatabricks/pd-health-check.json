{
  "checks": {
    "get-pd-data": {
      "type": "metric",
      "command": "python3.6 /etc/sensu/plugins/get-pd-data.py",
      "subscribers": [
        "roundrobin:vm"
      ],
      "interval": 1800,
      "source": "proxyclient",
      "occurrences": 1,
      "handlers": [
        "mailer"
      ]
    },
    "get-pd-check": {
      "type": "metric",
      "command": "python3.6 /etc/sensu/plugins/get-pd-check.py get-pd-check",
      "subscribers": [
        "roundrobin:vm"
      ],
      "resfresh": 2100,
      "interval": 2100,
      "source": "proxyclient",
      "occurrences": 1,
      "handlers": [
        "mailer",
        "ops_mailer2"
      ]
    }
  }
}