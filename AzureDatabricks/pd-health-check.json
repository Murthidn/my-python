{
  "checks": {
    "get-pd-data": {
      "type": "metric",
      "command": "python3.6 /etc/sensu/plugins/get-pd-data.py",
      "subscribers": [
        "sensu"
      ],
      "interval": 1800,
      "occurrences": 1,
      "handlers": [
        "mailer"
      ]
    },
    "get-pd-check": {
      "type": "metric",
      "command": "python3.6 /etc/sensu/plugins/get-pd-check.py",
      "subscribers": [
        "sensu"
      ],
      "interval": 2100,
      "occurrences": 1,
      "handlers": [
        "mailer"
      ]
    }
  }
}